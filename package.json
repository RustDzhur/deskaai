  "scripts": {
    "start": "node scripts/start.js",
    "start:prod": "cross-env REACT_APP_DEV_PROD=true REACT_APP_URL=app.deskaai.com REACT_APP_CHAT_IFRAME_URL=https://chat-window.deskaai.com DEV_PROXY=https://app.deskaai.com REACT_APP_SOCKET_URL=wss://app.deskaai.com/socket node scripts/start.js",
    "build": "node scripts/build.js",
    "postbuild": "mkdirp ../priv/static && cpx \"./build/**\" \"../priv/static\" --clean",
    "test": "node scripts/test.js",
    "deploy": "npm run build",
    "prettier": "prettier --write 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'",
    "prettier:check": "prettier --check 'src/**/*.{js,jsx,ts,tsx,json,css,scss,md}'"
  },
  "overrides": {
    "react-error-overlay": "6.0.9"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version",
      ">0.3%",
      "not ie 11",
      "not dead",
      "not op_mini all"
    ]
  },
  "proxy": "http://localhost:4000",
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "@types/jszip": "^3.4.1",
    "@types/lodash": "^4.14.159",
    "@types/node": "^12.0.0",
